<script type="text/javascript">

function catchOutboundClick(link) {
    link.onclick = function(e){
      e.preventDefault();
      trackOutboundLink(link, "Outbound Links", link.hostname, (link.target === '_blank' || e.ctrlKey || e.metaKey || e.which == 2));
    }
}

function trackOutboundLink(link, category, action, new_window) {
  try {
    _gaq.push(['_trackEvent', category , action]);
  } catch(err){}

  if(new_window){
    window.open(link.href, '_blank');
  } else {
    document.location.href = link.href;
  }
}

var outRe = new RegExp('^http:\/\/.+$','i');
var inRe = new RegExp('^http:\/\/'+location.hostname.replace("www.", "")+'.+$','i');
var links = document.getElementsByTagName("a");

for (var i = 0; i < links.length; i++) {
  var link = links[i];
  if (outRe.test(link.href) && !inRe.test(link.href)){
    catchOutboundClick(link);
  }
}
</script>


